@model ContatoModel
@{
    ViewData["Title"] = "Excluir Contato";
}

<div class="text-center">
    <h3>Deseja realmente apagar o contato (@Model.Nome)?</h3>
</div>

<div class="d-grid gap-2">
    <a class="btn btn-warning" role="button" asp-controller="Contato" asp-action="Index">Cancelar Exclusão</a>
    <a class="btn btn-danger" role="button" asp-controller="Contato" asp-route-id="@Model.Id" asp-action="Excluir">Confirmar Exclusão</a>
</div>
